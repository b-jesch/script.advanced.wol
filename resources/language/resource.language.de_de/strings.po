msgctxt "#51000"
msgid "Basic settings"
msgstr "Allgemeine Einstellungen"

msgctxt "#51001"
msgid "Ethernet MAC address"
msgstr "Ethernet MAC Adresse"

msgctxt "#51002"
msgid "IP address or hostname"
msgstr "IP Adresse oder Hostname"

msgctxt "#54000"
msgid "Autostart"
msgstr "Autostart"

msgctxt "#54001"
msgid "NOTE: Changing these settings requires a Kodi Restart!"
msgstr "ACHTUNG: Die Änderung dieser Einstellungen erfordert einen Kodi Neustart!"

msgctxt "#54010"
msgid "Wake on Kodi Launch"
msgstr "Wake-Up bei Start"

msgctxt "#54011"
msgid "Activate 'wake on launch'"
msgstr "'Wakep-Up bei Start' aktivieren"

msgctxt "#54012"
msgid "Delay WOL signal on 'wake on launch' (in seconds)"
msgstr "WOL Signal bei 'Wakep-Up bei Start' verzögern (Sekunden)"

msgctxt "#54020"
msgid "Also wake after Kodi returns from standby"
msgstr "Bei Rückkehr aus Standby Wake-Up ebenfalls ausführen"

msgctxt "#54021"
msgid "Activate 'wake after standby'"
msgstr "'Wake-Up nach Standby' aktivieren"

msgctxt "#54022"
msgid "Delay WOL signal on 'wake after standby' (in seconds)"
msgstr "WOL Signal bei 'Wake-Up nach Standby' verzögern (Sekunden)"

msgctxt "#54030"
msgid "Update libraries after 'wake on launch/after standby'"
msgstr "Bibliotheken nach erfolgreichem Aufwecken aktualisieren"

msgctxt "#54031"
msgid "Update video library"
msgstr "Video-Bibliothek aktualisieren"

msgctxt "#54032"
msgid "Update music library"
msgstr "Musik-Bibliothek aktualisieren"

msgctxt "#54033"
msgid "Delay library updates (in seconds)"
msgstr "Verzögere Bibliothek-Aktualisierungen (Sekunden)"

msgctxt "#54034"
msgid "Delay host up notification (in seconds)"
msgstr "Verzögere Host Up Benachrichtigung (Sekunden)"

msgctxt "#52000"
msgid "Notifications"
msgstr "Benachrichtigungen"

msgctxt "#52001"
msgid "Enable launch notification"
msgstr "Aktiviere Startbenachrichrigung"

msgctxt "#52002"
msgid "Enable ping or waiting counter notifications"
msgstr "Aktiviere Ping- oder Wartezeitbenachrichtigungen"

msgctxt "#52003"
msgid "Enable host up success/failure notification"
msgstr "Aktiviere Host Up Erfolgs-/Fehlerbenachrichtigung"

msgctxt "#52004"
msgid "Enable error notifications"
msgstr "Aktiviere Fehlerbenachrichtigungen"

msgctxt "#53000"
msgid "Advanced settings"
msgstr "Erweiterte Einstellungen"

msgctxt "#53010"
msgid "Host up check"
msgstr "Host Up Test"

msgctxt "#53011"
msgid "Ping timeout for host up check (in seconds)"
msgstr "Ping Timeout für Host Up Test (Sekunden)"

msgctxt "#53012"
msgid "Disable host up check via ping..."
msgstr "Deaktiviere Host Up Test via Ping..."

msgctxt "#53013"
msgid "...and simply assume wake up after this timespan (in seconds)"
msgstr "...und erwarte Aufwecken nach dieser Zeitspanne (Sekunden)"

msgctxt "#53020"
msgid "Continuous WOL signals"
msgstr "Kontinuierliche WOL-Signale"

msgctxt "#53021"
msgid "Send continuous WOL packets"
msgstr "Sende dauerhaft WOL-Signale"

msgctxt "#53022"
msgid "Delay between packets (in seconds)"
msgstr "Verzögerung zwischen den Paketen (Sekunden)"

msgctxt "#53023"
msgid "Continue sending after Kodi returns from standby"
msgstr "Sende dauerhaft nach der Rückkehr aus dem Standby"

msgctxt "#60000"
msgid "Waking up %s..."
msgstr "Wecke %s..."

msgctxt "#60001"
msgid "Waiting for %s to wake up..."
msgstr "Warte auf Verfügbarkeit von %s..."

msgctxt "#60002"
msgid "%s of %s seconds left"
msgstr "%s von %s Sekunden vergangen"

msgctxt "#60003"
msgid "%s did not wake up..."
msgstr "%s wacht nicht auf..."

msgctxt "#60004"
msgid "%s is awake"
msgstr "%s ist verfügbar"

msgctxt "#60005"
msgid "Ping successful, wait for %s seconds"
msgstr "Ping erfolgreich, warte noch %s Sekunden"

msgctxt "#60007"
msgid "Launching command..."
msgstr "Führe Befehl aus..."

msgctxt "#60008"
msgid "WOL packets will continue with a delay of %s seconds."
msgstr "WOL-Pakete werden fortlaufend mit einer Verzögerung von %s Sekunden gesendet."

msgctxt "#60011"
msgid "%s should be awake now!"
msgstr "%s sollte jetzt verfügbar sein!"
